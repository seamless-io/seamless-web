{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "command": [
                "gunicorn",
                "-w",
                "4",
                "-b",
                "0.0.0.0:5000",
                "--timeout",
                "120",
                "--log-level=info",
                "--capture-output",
                "--enable-stdio-inheritance",
                "application:app",
                "--reload"
            ],
            "environment": [
                {
                    "name": "PYTHONUNBUFFERED",
                    "value": "1"
                }
            ],
            "essential": true,
            "image": "202868668807.dkr.ecr.us-east-1.amazonaws.com/web-prod:web-prod-server",
            "memory": 128,
            "mountPoints": [
                {
                    "containerPath": "/src",
                    "sourceVolume": "_"
                }
            ],
            "name": "server",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 80
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "./"
            },
            "name": "_"
        }
    ]
}